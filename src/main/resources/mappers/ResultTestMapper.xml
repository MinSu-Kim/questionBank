<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yi.mapper.ResultTestMapper">

	<select id="selectByAll" resultType="ResultTestVO">
		SELECT *
		FROM resulttest
	</select>
	
	<select id="selectByNo" resultType="ResultTestVO">
		SELECT *
		FROM resulttest
		where resulttest_code=#{resultTestCode}
	</select>
	
	<select id="selectByCustomerCode" resultType="ResultTestVO">
		SELECT *
		FROM resulttest
		where customer=#{customer.customerCode}
	</select>
	
	<insert id="insertResultTest">
		
	</insert>
	
	<delete id="deleteResultTest">
		DELETE FROM resulttest
		WHERE resultTest_code=#{resultTestCode}
	</delete>
	
	<update id="updateResultTest">
		
	</update>

</mapper>






















